    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-jest": "^26.4.6",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.0",
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
